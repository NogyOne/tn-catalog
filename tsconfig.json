{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@public/*": [
        "public/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "@consts/*": [
        "src/consts/*"
      ],
      "@controllers/*": [
        "src/server/controllers/*"
      ],
      "@services/*":[
        "src/server/services/*"
      ],
      "@db/*": [
        "src/db/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}